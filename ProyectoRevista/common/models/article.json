{
  "name": "article",
  "plural": "Articles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "abstract": {
      "type": "string",
      "required": true
    },
    "keywords": {
      "type": "string",
      "required": true,
      "default": "Palabra clave;"
    },
    "authorName": {
      "type": "string",
      "required": true
    },
    "authorSurname": {
      "type": "string",
      "required": true
    },
    "authorEmail": {
      "type": "string",
      "required": true
    },
    "authorAfil": {
      "type": "string",
      "required": true
    },
    "articlePDf": {
      "type": "string"
    },
    "state": {
      "type": "string",
      "default": "Enviado"
    },
    "date": {
      "type": "Date"
    },
    "numEdition": {
      "type": "number",
      "required": true
    },
    "score": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}